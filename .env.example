# WaveSpeed API Configuration
WAVESPEED_API_KEY=your_api_key_here
WAVESPEED_API_HOST=https://api.wavespeed.ai

# MCP Configuration
WAVESPEED_MCP_BASE_PATH=~/Desktop
WAVESPEED_API_RESOURCE_MODE=url

# Logging Configuration
WAVESPEED_LOG_LEVEL=INFO
# Optional: Specify log file path (if not set, logs to console)
# WAVESPEED_LOG_FILE=/tmp/wavespeed-mcp.log
# WAVESPEED_LOG_FILE=/var/log/wavespeed-mcp.log
# WAVESPEED_LOG_FILE=~/logs/wavespeed-mcp.log

# Network Configuration
# Request timeout per HTTP call in seconds (default: 300 = 5 minutes)
# WAVESPEED_REQUEST_TIMEOUT=300

# Total timeout for waiting/polling results in seconds (default: 600 = 10 minutes)
# WAVESPEED_WAIT_RESULT_TIMEOUT=600