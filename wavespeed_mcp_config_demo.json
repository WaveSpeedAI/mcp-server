{
  "// MCP 服务器配置 - 用于 Claude Desktop 集成": "",
  "mcpServers": {
    "WaveSpeed": {
      "command": "wavespeed-mcp",
      "env": {
        "WAVESPEED_API_KEY": "your_api_key_here",
        "WAVESPEED_API_HOST": "https://api.wavespeed.ai",
        "WAVESPEED_MCP_BASE_PATH": "~/Desktop",
        "WAVESPEED_API_RESOURCE_MODE": "url",
        "WAVESPEED_LOG_LEVEL": "INFO"
      }
    }
  },

  "// WaveSpeed MCP 服务器配置": "",
  "api": {
    "key": "your_wavespeed_api_key_here",
    "host": "https://api.wavespeed.ai",
    "timeout": 60,
    "max_retries": -1,
    "retry_delay": 1,
    "polling_interval": 2,
    "endpoints": {
      "text_to_image": "/wavespeed-ai/flux-dev",
      "image_to_image": "/wavespeed-ai/flux-kontext-pro",
      "video": "/wavespeed-ai/wan-2.1/i2v-480p-lora"
    }
  },
  "output": {
    "base_path": "~/Desktop",
    "resource_mode": "url"
  },
  "logging": {
    "level": "INFO"
  },
  "telemetry": {
    "enabled": true
  }
}
